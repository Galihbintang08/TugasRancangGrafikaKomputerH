void jendela2() {

    //detail ruang lantai 2
    //tiang lantai 2===========================

    //tiang
    glPushMatrix();
    glutSolidCube(50.0);
    glTranslatef(180, 0, 200);
    glBegin(GL_POLYGON);
    glColor3f(0.95, 0.33, 0.06);
    glVertex3f(-10.0, 520.0, 300);
    glVertex3f(-10.0, 900.0, 300);
    glVertex3f(10.0, 900.0, 300);
    glVertex3f(10.0, 520.0, 300);
    glEnd();
    glPopMatrix();

