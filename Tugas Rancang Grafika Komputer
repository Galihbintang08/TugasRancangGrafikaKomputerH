    glVertex3f(-350.0, 1130.0, -165);
    glEnd();


}

void pagar_lantai2() {
    //garis pagar depan atas ruang 1======================================lantai2
    // depan
    glBegin(GL_POLYGON);
    glColor3f(0.21, 0.22, 0.23);
    glVertex3f(530.0, 200.0, 550);
    glVertex3f(530.0, 290.0, 550);
    glVertex3f(550.0, 290.0, 550);
    glVertex3f(550.0, 200.0, 550);
    glEnd();

    // belakang
    glBegin(GL_POLYGON);
