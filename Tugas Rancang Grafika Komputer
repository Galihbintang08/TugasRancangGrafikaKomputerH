    glEnd();

    // belakang
    glBegin(GL_POLYGON);
    glColor3f(1, 0.65, 0.32);
    glVertex3f(330.0, 1040.0, 165);
    glVertex3f(330.0, 1130.0, 165);
    glVertex3f(350.0, 1130.0, 165);
    glVertex3f(350.0, 1040.0, 165);
    glEnd();

    // atas
