    glEnd();
}

void pagar_lantai1() {
    //garis pagar depan atas ruang 1======================================lantai1
    // depan
    glBegin(GL_POLYGON);
    glColor3f(1, 0.65, 0.32);
    glVertex3f(530.0, 520.0, 550);
    glVertex3f(530.0, 590.0, 550);
    glVertex3f(550.0, 590.0, 550);
    glVertex3f(550.0, 520.0, 550);
    glEnd();

    // belakang
    glBegin(GL_POLYGON);
    glColor3f(1, 0.65, 0.32);
    glVertex3f(530.0, 520.0, 530);
    glVertex3f(530.0, 590.0, 530);
    glVertex3f(550.0, 590.0, 530);
    glVertex3f(550.0, 520.0, 530);
    glEnd();

    // atas
    glBegin(GL_POLYGON);
    glColor3f(1, 0.65, 0.32);
    glVertex3f(530.0, 590.0, 550);
    glVertex3f(530.0, 590.0, 530);
    glVertex3f(550.0, 590.0, 530);
    glVertex3f(550.0, 590.0, 550);
    glEnd();

    // bawah
    glBegin(GL_POLYGON);
    glColor3f(1, 0.65, 0.32);
    glVertex3f(530.0, 520.0, 550);
    glVertex3f(530.0, 520.0, 530);
    glVertex3f(550.0, 520.0, 530);
    glVertex3f(550.0, 520.0, 550);
    glEnd();

    // kiri
    glBegin(GL_POLYGON);
    glColor3f(1, 0.65, 0.32);
    glVertex3f(530.0, 590.0, 550);
    glVertex3f(530.0, 520.0, 550);
    glVertex3f(530.0, 520.0, 530);
