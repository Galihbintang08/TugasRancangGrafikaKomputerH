void jendela4(){
    //detail ruang paling atas=========================

        // depan=========
        //pintu
    glBegin(GL_POLYGON);
    glColor3f(0.4, 0.38, 0.38);
    glVertex3f(-80.0, 1350.0, 201);
    glVertex3f(-80.0, 1550.0, 201);
    glVertex3f(80.0, 1550.0, 201);
    glVertex3f(80.0, 1350.0, 201);
    glEnd();
    //pintu
    glBegin(GL_POLYGON);
    glColor3f(0.95, 0.33, 0.06);
    glVertex3f(-70.0, 1350.0, 202);
    glVertex3f(-70.0, 1540.0, 202);
    glVertex3f(70.0, 1540.0, 202);
    glVertex3f(70.0, 1350.0, 202);
    glEnd();

    //jendela bawah kiri
    glBegin(GL_POLYGON);
    glColor3f(0.68, 0.84, 0.91);
    glVertex3f(-180.0, 1350.0, 202);
    glVertex3f(-180.0, 1550.0, 202);
    glVertex3f(-90.0, 1550.0, 202);
    glVertex3f(-90.0, 1350.0, 202);
    glEnd();

    //jendela atas kiri
    glBegin(GL_POLYGON);
    glColor3f(0.88, 0.41, 0.0);
    glVertex3f(-175.0, 1460.0, 203);
    glVertex3f(-175.0, 1540.0, 203);
    glVertex3f(-95.0, 1540.0, 203);
    glVertex3f(-95.0, 1460.0, 203);
    glEnd();

    //jendela bawah kanan
    glBegin(GL_POLYGON);
    glColor3f(0.68, 0.84, 0.91);
    glVertex3f(180.0, 1350.0, 202);
    glVertex3f(180.0, 1550.0, 202);
    glVertex3f(90.0, 1550.0, 202);
    glVertex3f(90.0, 1350.0, 202);
    glEnd();

    //jendela atas kanan
    glBegin(GL_POLYGON);
    glColor3f(0.88, 0.41, 0.0);
    glVertex3f(175.0, 1460.0, 203);
    glVertex3f(175.0, 1540.0, 203);
