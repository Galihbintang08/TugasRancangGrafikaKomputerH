    glVertex3f(-350.0, 1130.0, 330);
    glVertex3f(350.0, 1130.0, 330);
    glVertex3f(350.0, 1130.0, 350);
    glEnd();

    // bawah
    glBegin(GL_POLYGON);
    glColor3f(1, 0.65, 0.32);
    glVertex3f(-350.0, 1110.0, 350);
    glVertex3f(-350.0, 1110.0, 330);
    glVertex3f(350.0, 1110.0, 330);
    glVertex3f(350.0, 1110.0, 350);
    glEnd();

