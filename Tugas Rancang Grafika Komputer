void jendela()
{
    //==========================================================
     //Jendela Pojok Kiri TEMBOK KIRI
    glBegin(GL_POLYGON);
    glColor3f(1.0, 1.0, 1.0);
    glVertex3f(-405.0, 220.0, 375);
    glVertex3f(-405.0, 420.0, 375);
    glVertex3f(-405.0, 420.0, 250);
    glVertex3f(-405.0, 220.0, 250);
    glEnd();

    glBegin(GL_POLYGON);
    glColor3f(1.0, 1.0, 1.0);
    glVertex3f(-405.0, 430.0, 375);
    glVertex3f(-405.0, 480.0, 375);
    glVertex3f(-405.0, 480.0, 250);
    glVertex3f(-405.0, 430.0, 250);
    glEnd();
   
    //Motif Jendela Pojok Kiri TEMBOK KIRI
    //tegak kiri
    glBegin(GL_POLYGON);
    glColor3f(0.1, 0.1, 0.1);
    glVertex3f(-407, 230.0, 365.0);
    glVertex3f(-407, 370.0, 365.0);
    glVertex3f(-407, 370.0, 360.0);
    glVertex3f(-407, 230.0, 360.0);
    glEnd();
    //miring kiri
    glBegin(GL_QUADS);
    glColor3f(0.1, 0.1, 0.1);
    glVertex3f(-407, 370.0, 365.0);
    glVertex3f(-407, 410.0, 330.0);
    glVertex3f(-407, 400.0, 330.0);
    glVertex3f(-407, 370.0, 360.0);
    glEnd();
    //tegak kanan
    glBegin(GL_POLYGON);
    glColor3f(0.1, 0.1, 0.1);
    glVertex3f(-407, 230.0, 260.0);
    glVertex3f(-407, 370.0, 260.0);
    glVertex3f(-407, 370.0, 265.0);
    glVertex3f(-407, 230.0, 265.0);
    glEnd();
    //miring kanan
    glBegin(GL_QUADS);
    glColor3f(0.1, 0.1, 0.1);
    glVertex3f(-407, 370.0, 260.0);
    glVertex3f(-407, 410.0, 295.0);
    glVertex3f(-407, 400.0, 295.0);
    glVertex3f(-407, 370.0, 265.0);
    glEnd();
    //Atasdatar
    glBegin(GL_QUADS);
    glColor3f(0.1, 0.1, 0.1);
    glVertex3f(-407, 400.0, 330.0);
    glVertex3f(-407, 400.0, 295.0);
    glVertex3f(-407, 410.0, 295.0);
    glVertex3f(-407, 410.0, 330.0);
    glEnd();
    //Bawahdatar
    glBegin(GL_QUADS);
    glColor3f(0.1, 0.1, 0.1);
    glVertex3f(-407, 225.0, 365.0);
    glVertex3f(-407, 225.0, 260.0);
    glVertex3f(-407, 230.0, 260.0);
    glVertex3f(-407, 230.0, 365.0);
    glEnd();
    //motifgarisgaristengahtegak
    glBegin(GL_QUADS);
    glColor3f(0.1, 0.1, 0.1);
    glVertex3f(-408, 230.0, 300.0);
    glVertex3f(-408, 230.0, 295.0);
    glVertex3f(-408, 400.0, 295.0);
    glVertex3f(-408, 400.0, 300.0);
    glEnd();
    glBegin(GL_QUADS);
    glColor3f(0.1, 0.1, 0.1);
    glVertex3f(-408, 230.0, 330.0);
    glVertex3f(-408, 230.0, 335.0);
    glVertex3f(-408, 400.0, 335.0);
    glVertex3f(-408, 400.0, 330.0);
    glEnd();
    //motifgarisgaristengahdatar
    glBegin(GL_QUADS);
    glColor3f(0.1, 0.1, 0.1);
    glVertex3f(-408, 365.0, 360.0);
    glVertex3f(-408, 365.0, 265.0);
    glVertex3f(-408, 370.0, 265.0);
    glVertex3f(-408, 370.0, 360.0);
    glEnd();
    glBegin(GL_QUADS);
    glColor3f(0.1, 0.1, 0.1);
    glVertex3f(-408, 335.0, 360.0);
    glVertex3f(-408, 335.0, 265.0);
    glVertex3f(-408, 340.0, 265.0);
    glVertex3f(-408, 340.0, 360.0);
    glEnd();
    glBegin(GL_QUADS);
    glColor3f(0.1, 0.1, 0.1);
    glVertex3f(-408, 315.0, 360.0);
    glVertex3f(-408, 315.0, 265.0);
    glVertex3f(-408, 320.0, 265.0);
    glVertex3f(-408, 320.0, 360.0);
    glEnd();
    glBegin(GL_QUADS);
    glColor3f(0.1, 0.1, 0.1);
    glVertex3f(-408, 295.0, 360.0);
    glVertex3f(-408, 295.0, 265.0);
    glVertex3f(-408, 300.0, 265.0);
    glVertex3f(-408, 300.0, 360.0);
    glEnd();
    glBegin(GL_QUADS);
    glColor3f(0.1, 0.1, 0.1);
    glVertex3f(-408, 275.0, 360.0);
    glVertex3f(-408, 275.0, 265.0);
