void pagar_lantai3(){
  
    //Lantai ruang 3===============================================

        // depan
    glBegin(GL_POLYGON);
    glColor3f(1, 0.65, 0.32);
    glVertex3f(-350.0, 1110.0, 350);
    glVertex3f(-350.0, 1130.0, 350);
    glVertex3f(350.0, 1130.0, 350);
    glVertex3f(350.0, 1110.0, 350);
    glEnd();

    // belakang
    glBegin(GL_POLYGON);
    glColor3f(1, 0.65, 0.32);
